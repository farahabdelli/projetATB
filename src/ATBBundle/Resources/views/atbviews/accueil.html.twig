{% extends 'base3.html.twig' %}
{% block content %}
<br><br>


    <h3 class="subtitle fancy"><span><a href="{{ path('afficherCompte') }}"> Mes comptes</a></span></h3>
    <br><br>
    <table class="table table-striped table-hover ">
        <thead>
        <tr>

            <th>N° compte</th>
            <th>Type de compte</th>
            <th>Devise</th>
            <th>Solde actuel</th>
            <th>Solde disponible</th>
            <th>Solde actuel en TND</th>

        </tr>
        </thead>
        <tbody>

        {% for i in em %}
            <tr>

                <td><a href="{{ absolute_url(asset('')) }}app_dev.php/atb/afficherDetail/{{ i.numCompte }}"  >{{ i.numCompte }}</a> </td>
                <td>{{ i.typeCompte }}  </td>
                <td>{{ i.deviseCompte }}</td>
                <td>{{ i.soldeActuel }}</td>
                <td>{{ i.soldeDisponible }}</td>
                <td>{{ i.soldeActueltnd }}</td>


            </tr>
        {% endfor %}
        </tbody>
    </table>

    <br><br><br>

    <h3 class="subtitle fancy"><span><a href="{{ path('afficherCarte') }}"> Mes cartes</a></span></h3>

    <br><br>
    <table class="table table-striped table-hover ">
        <thead>
        <tr>

            <th>N° carte</th>
            <th>Type de carte</th>
            <th>N° compte</th>
            <th>Porteur</th>
            <th>Plafond</th>
            <th>Date d'expiration</th>
            <th>Statut</th>


        </tr>
        </thead>
        <tbody>

        {% for i in m %}
            <tr>

                <td>{{ i.numCarte }}</td>
                <td>{{ i.typeCarte }}</td>
                <td>{{ i.numCompte.numCompte }}</td>
                <td>{{ i.porteur }}</td>
                <td>{{ i.plafond }}</td>
                <td>{{ i.dateExpiration }}</td>
                <td>{{ i.statut }}</td>







            </tr>
        {% endfor %}
        </tbody>
    </table>

    <br><br>


{% endblock %}
